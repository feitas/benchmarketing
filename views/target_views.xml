<odoo>
	<data>

		<record id="bench_marketing_target_view_tree" model="ir.ui.view">
	        <field name="name">bench.marketing.target.tree</field>
	        <field name="model">bench.marketing.target</field>
	        <field name="arch" type="xml">
	            <tree>
	                <field name="name"/>
	                <field name="partner_id"/>
	            </tree>
	        </field>
    	</record>

    	<record id="bench_marketing_target_view_form" model="ir.ui.view">
	        <field name="name">bench.marketing.target.view.form</field>
	        <field name="model">bench.marketing.target</field>
	        <field name="arch" type="xml">
	            <form string="Team">
	                <sheet>
	                    <group>
	                        <group>
	                            <field name="name"/>
	                            <field name="partner_id"/>
	                        </group>
	                   	
	                    </group>
	                    <notebook>
	                    	<page string="Analyse Content">
	                    		<field name="content" />
	                    	</page>
	                    	<page string="Scheme" >
	                    		<field name="scheme" />
	                    	</page>
	                    </notebook>
	                </sheet>
	            </form>
	        </field>
	    </record>

	</data>
</odoo>
